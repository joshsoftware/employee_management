<div id = "wrapper">
  <%= render :partial => '/layouts/admin/top' %>
  <div id = "container">
    <%= render :partial => '/plugins/category' %>
    <div id = "rightinner">
      <div id = "titlegreenleft">
        <div class="greentitle"><img src="/images/content.png" style="vertical-align:middle; margin-right:8px;" />Departments</div>
      </div>
      <br/><br/>
      <% form_for @department, :url =>  departments_path, :method => :post do |department| %>
        <div id = "matterinner" style="height:350px;">
          <tr>
            <td colspan="3" align="left" valign="top" bgcolor="#cfeb79"><div class="greentitle">Department - Add New Department</div></td>
          </tr>
          <%= render :partial => 'form', :locals => { :department => department } %>
        <% end %>
      </div>
    </div>
  </div>
  <div style="clear:both"></div>
</div><br/><br/><br/>
  </div>

