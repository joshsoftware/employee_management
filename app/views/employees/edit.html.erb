<div id = "wrapper">
  <%= render :partial => '/layouts/admin/top' %>
  <div id = "container">
    <%= render :partial => '/plugins/category' %>
    <div id = "rightinner">
      <div id = "titlegreenleft">
        <div class="greentitle"><img src="/images/content.png" style="vertical-align:middle; margin-right:8px;" />Departments</div>
      </div>
      <br/>
      <%= error_messages_for 'employee' %><br/>
      <% form_for @employee, :url =>  employee_path(@employee), :method => :put do |form| %>
        <div id = "matterinner" style="height:350px;">
          <table  width="" border="0" >
            <tr>
              <td >
                <%= render :partial => 'form' %>
              </td>
            </tr>
            <tr>
              <td align="center" valign="middle" class="tablelinks">
                <div class="greenbut" style="margin-left:0px;"><%= submit_tag 'CREATE', :class => "greensubbut"%></div>
                <div class="greenbut" style="margin-left:20px;"><%= link_to 'CANCEL', employees_path %></div>	</td>
            </tr>
          </table>
        </div>
      <% end %>
    </div>
  </div>
  <div style="clear:both"></div>
</div><br/><br/><br/>
  </div>



